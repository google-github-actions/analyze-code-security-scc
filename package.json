{
  "name": "@google-github-actions/analyze-code-security-scc",
  "version": "0.0.0",
  "description": "Provides functionality to scan code for vulnerability during the build step of the project.",
  "main": "dist/main/index.js",
  "scripts": {
    "build": "ncc build -m src/main.ts -o dist/main",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write **/*.ts",
    "test": "node --require ts-node/register --test-reporter spec --test test/accessor.test.ts test/iac_scan_report_processor.test.ts test/utils.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/google-github-actions/analyze-code-security-scc"
  },
  "keywords": [
    "actions",
    "google cloud",
    "code scanning",
    "vulnerability detection"
  ],
  "author": "GoogleCloudPlatform",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/http-client": "^2.2.0",
    "@google-github-actions/actions-utils": "^0.7.0",
    "google-auth-library": "^9.4.2"
  },
  "devDependencies": {
    "@types/sinon": "^17.0.1",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "sinon": "^17.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
